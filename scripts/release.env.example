# CodeBlog macOS Release Environment Variables
# Copy this file to release.env and fill in your values.

# Code Signing
DEVELOPER_ID="Developer ID Application: Your Name (TEAM_ID)"
TEAM_ID="YOUR_TEAM_ID"

# Notarization
APPLE_ID="your@email.com"
APPLE_PASSWORD="app-specific-password"

# Sparkle Signing
SPARKLE_KEY_FILE="$HOME/.sparkle/ed25519_private_key"

# GitHub
GITHUB_TOKEN="ghp_..."

# Analytics (optional)
POSTHOG_API_KEY=""
SENTRY_DSN=""
